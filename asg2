#include <stdio.h>

#define rows 3
#define cols 4

int main() 
{
    int array[rows][cols];
    
    for (int i = 0; i < rows; i++) 
    {
        for (int j = 0; j < cols; j++)
        {
            scanf("%d", &array[i][j]);
        }
    }

    
    int *ptr = &array[0][0];

    int sum_odd_row_even_col = 0;
    int sum_even_row_divisible_by_3 = 0;
    int sum_row_divisible_by_3_odd_col = 0;

    for (int i = 0; i < rows; i++)
    {
        for (int j = 0; j < cols; j++) 
        {
            int val = *(ptr+i*cols+j);
            if ((i & 1) && (i % 3 != 0) && !(j & 1) && (j % 3 != 0)) 
            {
                sum_odd_row_even_col += val;
            }
            if (!(i & 1) && (i % 3 != 0) && (j % 3 == 0)) 
            {
                sum_even_row_divisible_by_3 += val;
            }
            if ((i % 3 == 0) && (j & 1) && (j % 3 != 0))
            {
                sum_row_divisible_by_3_odd_col += val;
            } 
        }
    }


    printf("sum of elements with row index odd but not divisible by 3, column index even but not divisible by 3: %d\n", sum_odd_row_even_col);
    printf("Sum of elements with row index even but not divisible by 3, column index only divisible by 3: %d\n", sum_even_row_divisible_by_3);
    printf("Sum of elements with row index divisible only by 3, and column index odd but not divisible by 3: %d\n", sum_row_divisible_by_3_odd_col);

    return 0;
}
